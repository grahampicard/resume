{{template "layout" .}}

{{define "title"}}
	Projects
{{end}}

{{define "content"}}
<div class="projects">
    <div class="projects-heading">
        <h1>Selected Portfolio</h1>
    </div>
    <div class="projects-list">
    <section>
      {{range .Portfolio}}
      <a href="projects/{{.ID}}">{{.Name}}</a>
      {{ end }}
    </section>
    </div>
</div>
{{end}}
